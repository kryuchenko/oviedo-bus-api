<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/rowSearchStop"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tvNameSearchStop"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/spacing_normal"
        android:fontFamily="@font/helveticaneue_medium"
        app:layout_constraintBottom_toTopOf="@+id/tvCodeSearchStop"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        style="@style/TextViewStyle.TextViewStyleRegular"/>
    <TextView
        android:textSize="14sp"
        android:id="@+id/tvCodeSearchStop"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="5dp"
        android:layout_marginBottom="@dimen/spacing_small"
        android:fontFamily="@font/helveticaneue_light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="@+id/tvNameSearchStop"
        app:layout_constraintStart_toStartOf="@+id/tvNameSearchStop"
        app:layout_constraintTop_toBottomOf="@+id/tvNameSearchStop"/>
</androidx.constraintlayout.widget.ConstraintLayout>
