<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:background="#ffffff"
    android:fitsSystemWindows="true"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:textSize="24sp"
        android:textStyle="bold"
        android:textColor="@color/black"
        android:gravity="center"
        android:id="@+id/txt_seleccion"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="58dp"
        android:layout_marginTop="64dp"
        android:text="@string/seleccion_doc_es"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:background="@drawable/border"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="103dp"
        android:layout_marginTop="68dp"
        android:layout_marginStart="20dp"
        android:layout_marginEnd="20dp"
        app:layout_constraintEnd_toEndOf="@+id/txt_seleccion"
        app:layout_constraintStart_toStartOf="@+id/txt_seleccion"
        app:layout_constraintTop_toBottomOf="@+id/txt_seleccion">
        <ImageButton
            android:id="@+id/icon_dni"
            android:background="@null"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="0x7f0800e7"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.162"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <Button
            android:textSize="14sp"
            android:textColor="@color/black"
            android:id="@+id/icon_dni2"
            android:background="@null"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="@string/dni"
            android:textAllCaps="false"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="16dp"
            app:backgroundTint="@android:color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/icon_dni"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_goneMarginStart="8dp"
            style="?android:attr/borderlessButtonStyle"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:background="@drawable/border"
        android:visibility="gone"
        android:layout_width="0dp"
        android:layout_height="103dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="@+id/constraintLayout"
        app:layout_constraintStart_toStartOf="@+id/constraintLayout"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout">
        <ImageButton
            android:id="@+id/icon_passport"
            android:background="@null"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="0x7f08013b"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.162"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"/>
        <Button
            android:textSize="14sp"
            android:textColor="@color/black"
            android:id="@+id/icon_passport2"
            android:background="@null"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:text="Passport"
            android:textAllCaps="false"
            android:layout_marginStart="32dp"
            android:layout_marginEnd="16dp"
            app:backgroundTint="@android:color/white"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/icon_passport"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_goneMarginStart="8dp"
            style="?android:attr/borderlessButtonStyle"/>
    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
